<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn"
             targetNamespace="Examples">

    <process id="publishProcess" name="Content Publish Process" isExecutable="true">
        <startEvent id="start" />
        <sequenceFlow sourceRef="start" targetRef="initSocialTask" />

        <serviceTask id="initSocialTask" name="Initialize Social Data"
                     activiti:delegateExpression="${socialInitDelegate}" />

        <sequenceFlow sourceRef="initSocialTask" targetRef="end" />
        <endEvent id="end" />
    </process>
</definitions>