<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             targetNamespace="Order1">
    <process id="process_square_first" name="先平方后取反" isExecutable="true">
        <startEvent id="start" name="开始" />
        <serviceTask id="task_square" name="平方计算" activiti:delegateExpression="${squareDelegate}" />
        <serviceTask id="task_negate" name="取反计算" activiti:delegateExpression="${negateDelegate}" />
        <endEvent id="end" name="结束" />
        <sequenceFlow id="flow1" sourceRef="start" targetRef="task_square" />
        <sequenceFlow id="flow2" sourceRef="task_square" targetRef="task_negate" />
        <sequenceFlow id="flow3" sourceRef="task_negate" targetRef="end" />
    </process>
    <bpmndi:BPMNDiagram id="BPMNDiagram_math">
        <bpmndi:BPMNPlane bpmnElement="process_square_first" id="BPMNPlane_math">
            <bpmndi:BPMNShape bpmnElement="start" id="BPMNShape_start">
                <omgdc:Bounds height="35.0" width="35.0" x="50.0" y="100.0" />
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="task_square" id="BPMNShape_task_square">
                <omgdc:Bounds height="55.0" width="105.0" x="130.0" y="90.0" />
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="task_negate" id="BPMNShape_task_negate">
                <omgdc:Bounds height="55.0" width="105.0" x="280.0" y="90.0" />
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="end" id="BPMNShape_end">
                <omgdc:Bounds height="35.0" width="35.0" x="430.0" y="100.0" />
            </bpmndi:BPMNShape>
            <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
                <omgdi:waypoint x="85.0" y="117.0" />
                <omgdi:waypoint x="130.0" y="117.0" />
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
                <omgdi:waypoint x="235.0" y="117.0" />
                <omgdi:waypoint x="280.0" y="117.0" />
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
                <omgdi:waypoint x="385.0" y="117.0" />
                <omgdi:waypoint x="430.0" y="117.0" />
            </bpmndi:BPMNEdge>
        </bpmndi:BPMNPlane>
    </bpmndi:BPMNDiagram>
</definitions>